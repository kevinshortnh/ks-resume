/TJ { % Similar but not equal to PDF's TJ operator
  {
    dup type /stringtype eq
    { show }
    { neg 1000 div 0 rmoveto }
    ifelse
  } forall
} bd
/ATJ { % As TJ but adds letter-spacing
  /ATJls exch def
  {
    dup type /stringtype eq
    { ATJls 0 3 2 roll ashow }
    { neg 1000 div 0 rmoveto }
    ifelse
  } forall
} bd
